begin
char chooseSymbol1() is
    println "========= Tic Tac Toe ================" ;
    println "=  Because we know you want to win   =" ;
    println "======================================" ;
    println "=                                    =" ;
    println "= Who would you like to be?          =" ;
    println "=   x  (play first)                  =" ;
    println "=   o  (play second)                 =" ;
    println "=   q  (quit)                        =" ;
    println "=                                    =" ;
    println "======================================" ;
    
    char chosen = '\0' ;
    while chosen == '\0' do 
        print "Which symbol you would like to choose: " ;
        char c = '\0' ;
        read c ;
        if c == 'x' || c == 'X' then
            chosen = 'x'
        else
            if c == 'o' || c == 'O' then
                chosen = 'o'
            else
                if c == 'q' || c == 'Q' then
                    println "Goodbye safety." ;
                    exit 0
                else
                    print "Invalid symbol: " ;
                    println c ;
                    println "Please try again."
                fi 
            fi
        fi
    done ;
    print "You have chosen: " ;
    println chosen ;
    return chosen
end

